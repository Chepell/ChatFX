<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="100.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="chat.AboutController">
    <children>
        <VBox prefHeight="100.0" prefWidth="200.0">
            <children>
                <HBox prefHeight="16.0" prefWidth="73.0">
                    <children>
                        <Label prefHeight="25.0" prefWidth="75.0" text="Author:">
                            <font>
                                <Font size="16.0"/>
                            </font>
                        </Label>
                        <Label fx:id="authorLabel" prefHeight="25.0" prefWidth="123.0" text="Vasiya Pupkin">
                            <font>
                                <Font size="16.0"/>
                            </font>
                            <opaqueInsets>
                                <Insets/>
                            </opaqueInsets>
                        </Label>
                    </children>
                </HBox>
                <HBox prefHeight="16.0" prefWidth="73.0">
                    <children>
                        <Label prefHeight="25.0" prefWidth="75.0" text="Version:">
                            <font>
                                <Font size="16.0"/>
                            </font>
                        </Label>
                        <Label fx:id="versionLabel" prefHeight="25.0" prefWidth="123.0" text="0.11">
                            <font>
                                <Font size="16.0"/>
                            </font>
                            <opaqueInsets>
                                <Insets/>
                            </opaqueInsets>
                        </Label>
                    </children>
                </HBox>
                <HBox prefHeight="16.0" prefWidth="73.0">
                    <children>
                        <Label prefHeight="25.0" prefWidth="75.0" text="Year:">
                            <font>
                                <Font size="16.0"/>
                            </font>
                        </Label>
                        <Label fx:id="yearLabel" prefHeight="25.0" prefWidth="123.0" text="2019">
                            <font>
                                <Font size="16.0"/>
                            </font>
                            <opaqueInsets>
                                <Insets/>
                            </opaqueInsets>
                        </Label>
                    </children>
                </HBox>
            </children>
            <padding>
                <Insets left="10.0" top="10.0"/>
            </padding>
        </VBox>
    </children>
</AnchorPane>
